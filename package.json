{
  "name": "badmintonslotchecker",
  "version": "1.0.0",
  "description": "Checking if badminton slots are open",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "forever start index.js",
    "forever-list": "forever list",
    "forever-stop": "forever stop",
    "cypress:open": "cypress open",
    "cypress:sample": "cypress run --spec ./cypress/integration/examples/window.spec.js",
    "monday": "cypress run --spec ./src/cypress/monday.js",
    "tuesday": "cypress run --spec ./src/cypress/tuesday.js",
    "wednesday": "cypress run --spec ./src/cypress/wednesday.js",
    "thursday": "cypress run --spec ./src/cypress/thursday.js",
    "friday": "cypress run --spec ./src/cypress/friday.js",
    "saturday": "cypress run --spec ./src/cypress/saturday.js",
    "sunday": "cypress run --spec ./src/cypress/sunday.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.10.4",
    "forever": "^3.0.4",
    "jsdom": "^16.4.0",
    "nodemailer": "^6.5.0"
  },
  "devDependencies": {
    "cypress": "^6.6.0"
  }
}
